{
  "name": "foglet-ndp",
  "version": "3.0.1",
  "description": "This project aims to provide a simple neighbours delegated protocol and implement various SPARQL query delegation protocols",
  "main": "foglet-ndp.js",
  "scripts": {
    "doc": "jsdoc -c .jsdoc.json README.md src/",
    "test": "npm run karma",
    "karma": "karma start karma.config.js",
    "build:ndp": "browserify -r ./foglet-ndp.js:foglet-ndp  -t [ babelify --presets [ es2015 ] ]   > ./build/foglet-ndp.bundle.js",
    "build:watch": "watchify -r ./foglet-ndp.js:foglet-ndp  -t [ babelify --presets [ es2015 ] ]  -o ./build/foglet-ndp.bundle.js -v -d ",
    "signaling": "node node_modules/foglet-signaling-server/express-signaling-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/folkvir/foglet-ndp.git"
  },
  "keywords": [
    "spray-wrtc",
    "foglet-core",
    "neighbours",
    "delegated",
    "delegation",
    "protocol",
    "ldf",
    "SPARQL",
    "query delegation"
  ],
  "author": "Grall Arnaud (Folkvir)",
  "contributors": [
    "Thomas Minier <thomas.minier@protonmail.com"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/folkvir/foglet-ndp/issues"
  },
  "homepage": "https://github.com/folkvir/foglet-ndp#readme",
  "dependencies": {
    "debug": "^2.6.3",
    "foglet-core": "^2.1.0",
    "immutable": "^3.8.1",
    "ldf-client": "git://github.com/folkvir/Client.js.git",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "q": "^1.4.1",
    "regression": "^1.4.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "browserify-istanbul": "^2.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "foglet-signaling-server": "^0.3.0",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-express-http-server": "0.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-socketio-server": "^1.0.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "watchify": "^3.7.0"
  },
  "bundledDependencies": [
    "debug",
    "foglet-core",
    "immutable",
    "ldf-client",
    "moment",
    "q",
    "uuid"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
